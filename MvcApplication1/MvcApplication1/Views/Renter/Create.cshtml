@model MvcApplication1.Models.Renter
@{
    ViewBag.Title = "Create";
}

<h2>Create</h2>
@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    @Html.ValidationSummary()
     <fieldset>
        <legend>Registration Form</legend>
        <ol>
  <li>
       
                @Html.LabelFor(m => m.UserName)
                @Html.TextBoxFor(m => m.UserName)
            </li>
            <li>
                @Html.LabelFor(m => m.Password)
                @Html.PasswordFor(m => m.Password)
            </li>
            <li>
                @Html.LabelFor(m => m.ConfirmPassword)
                @Html.PasswordFor(m => m.ConfirmPassword)
            </li>
            <li>
                @Html.LabelFor(m => m.Name)
                @Html.TextBoxFor(m => m.Name)
            </li>
             <li>
                @Html.LabelFor(m => m.SirName)
                @Html.TextBoxFor(m => m.SirName)
            </li>
             <li>
                @Html.LabelFor(m => m.Address)
                @Html.TextBoxFor(m => m.Address)
            </li>
             <li>
                @Html.LabelFor(m => m.PostTown)
                @Html.TextBoxFor(m => m.PostTown)
            </li>
             <li>
                @Html.LabelFor(m => m.ZipCode)
                <input type="number" name="RegisterModel.ZipCode" min="10000" max="99999" /><br />
            </li>
             <li>
                @Html.LabelFor(m => m.Website)
                @Html.TextBoxFor(m => m.Website)
            </li>
             <li>
                @Html.LabelFor(m => m.TelephoneNumber)
                @Html.TextBoxFor(m => m.TelephoneNumber)
            </li>
             <li>
                @Html.LabelFor(m => m.email)
                @Html.TextBoxFor(m => m.email)
            </li>
             <li>
                @Html.LabelFor(m => m.AccountNumber)
                @Html.TextBoxFor(m => m.AccountNumber)
            </li>
                   
        </ol>
        <input type="submit" value="Register" />
    </fieldset>
}

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
